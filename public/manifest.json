{
    "manifest_version": 3,
    "name": "我的 Chrome 扩展",
    "version": "1.0.0",
    "side_panel": {
        "default_path": "side-panel.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "permissions": [
        "activeTab",
        "sidePanel",
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.baidu.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}